   RowBox[{
    RowBox[{
     RowBox[{"IGLibLoadAssemblies", "[", "assemblyNames_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"num", ",", "i", ",", "name"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"num", " ", "=", " ", 
         RowBox[{"Length", "[", "assemblyNames", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", " ", "=", " ", "1"}], ",", 
          RowBox[{"i", "\[LessEqual]", "num"}], ",", 
          RowBox[{"i", "=", 
           RowBox[{"i", "+", "1"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"name", "=", 
            RowBox[{"assemblyNames", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"(*", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Print", "[", 
              RowBox[{
              "\"\<Loading IGLib assembly \>\"", ",", "name", ",", 
               "\"\<...\>\""}], "]"}], ";"}], "\[IndentingNewLine]", "*)"}], 
           "\[IndentingNewLine]", 
           RowBox[{"IGLibLoadAssembly", "[", "name", "]"}]}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
     "Returns", " ", "IGLib", " ", "type", " ", "loaded", " ", "from", " ", 
      "the", " ", "specified", " ", "IGLib", " ", "assembly"}], ";", " ", 
     RowBox[{
     "assembly", " ", "can", " ", "be", " ", "specified", " ", "by", " ", 
      "the", " ", "assembly", " ", "file", " ", "name", " ", "or", " ", "by", 
      " ", 
      RowBox[{"(", "loaded", ")"}], " ", "assembly", " ", 
      RowBox[{"object", ":"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
