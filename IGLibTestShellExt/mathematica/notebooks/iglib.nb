        RowBox[{"LoadNETType", "[", "\"\<System.Environment\>\"", "]"}], ";", 
        
        RowBox[{"System`Environment`Version", "@", 
         RowBox[{"ToString", "[", "]"}]}]}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"IGLibLoadAssembly", "[", "assemblyName_", "]"}], ":=", 
     RowBox[{"LoadNETAssembly", "[", 
      RowBox[{"IGLibAssemblyPath", "[", "assemblyName", "]"}], "]"}]}], ";"}],
    "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"IGLidLoadAssemblies", "[", "]"}], ":=", 
     RowBox[{"IGLibLoadAssemblies", "[", "IGLibSelectedAssemblies", "]"}]}], 
