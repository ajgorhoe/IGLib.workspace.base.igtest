Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Installation", " ", 
    RowBox[{"of", " ", ".", "NET"}], " ", 
    RowBox[{"runtime", ":"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
