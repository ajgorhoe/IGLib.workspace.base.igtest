   RowBox[{
    RowBox[{
     RowBox[{"IGLibType", "[", "typeName_", "]"}], ":=", 
     RowBox[{"LoadNETType", "[", 
      RowBox[{"typeName", ",", 
       RowBox[{"AllowShortContext", "->", "IGLibUseShortNames"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"IGLibTypes", "[", "typeNames_", "]"}], ":=", 
     RowBox[{"Module", "[", 
