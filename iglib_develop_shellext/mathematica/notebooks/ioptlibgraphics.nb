Cell[TextData[{
 "show2d, show3d",
 StyleBox[" [ graphics_, useroptions_, pointsize_, thickness_, color_, \
showplot_ ]\n  ",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["- shows 2D graphics with requested changes in sgraphic options, \
point sizes, line thicknesses, and colors.\n  ",
  FontWeight->"Plain"],
 StyleBox["graphics",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" - 2D graphics or list of graphics\n  ",
  FontWeight->"Plain"],
 StyleBox["useroptions",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" - a list (possibly empty) of graphic options for ",
  FontWeight->"Plain"],
 StyleBox["Show",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" specified by the user (they override default options, which are \
already specified and are printed if plotting is requested)\n  ",
  FontWeight->"Plain"],
 StyleBox["pointsize",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" - if different than 0 or { } then size of points in graph is \
changed to the size(s) specified by  ",
  FontWeight->"Plain"],
 StyleBox["pointsize",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[". If this is a list numbers then elemets apply to elements of ",
  FontWeight->"Plain"],
 StyleBox["graphics",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[", if it is only one element or a list with 1 number then this size \
applies to all elements of ",
  FontWeight->"Plain"],
 StyleBox["graphics",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[". To leave point size in a particular element of ",
  FontWeight->"Plain"],
 StyleBox["graphics",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" unchanged then put {} or 0 on the desired place.\n  ",
  FontWeight->"Plain"],
 StyleBox["thickness",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" - defines requested line thickness plotted in graph. The same \
rules apply as for ",
  FontWeight->"Plain"],
 StyleBox["pointsize",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[".\n  ",
  FontWeight->"Plain"],
 StyleBox["color",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" - defines new color for lines AND points in the graph. The same \
rules apply as for ",
  FontWeight->"Plain"],
 StyleBox["pointsize",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[".\n  ",
  FontWeight->"Plain"],
 StyleBox["showplot",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" - if 0 then graph is not actually plotted, only ",
  FontWeight->"Plain"],
 StyleBox["graphics",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" with the requested changes in point sizes, line thicknesses and \
colors is returned.\n  ",
  FontWeight->"Plain"],
 "Returns",
 StyleBox[" graphics with eventual changes (e.g. in point size) applied\n  \n\
",
  FontWeight->"Plain"],
 "show3d",
 StyleBox[" - the same as show2d, but for 3D graphics. The only difference in \
use is that ",
  FontWeight->"Plain"],
 StyleBox["useroptions",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" must be appropriate for 3D graphics.\n\n",
  FontWeight->"Plain"],
 "showbas",
 StyleBox[" [ ..., <gr3d> ]",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["  - either the function of show2d or show3d (... refers to the \
