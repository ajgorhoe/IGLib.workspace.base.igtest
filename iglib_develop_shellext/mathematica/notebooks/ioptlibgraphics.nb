Cell[TextData[{
 "handlecolor",
 StyleBox[" [ col ] ",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" converts col to a valid color. \n",
  FontWeight->"Plain"],
 StyleBox["col",
  FontSize->12,
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["   a valid color specification such as Hue[0.5] or \
RGBColor[1,0,0.5]: col is returned. \n",
  FontSize->12,
  FontWeight->"Plain"],
 StyleBox["col",
  FontSize->12,
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["  an integer greater than or equal to 1 - a color is picked from a \
global palette (index overflow is handled internally).\n",
  FontSize->12,
  FontWeight->"Plain"],
 StyleBox["col",
  FontSize->12,
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" a number between 0 and 1 - Hue[col] is returned\n",
  FontSize->12,
  FontWeight->"Plain"],
 StyleBox["col",
  FontSize->12,
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" a list of three numbers - RGBColor[ col[[1]], col[[2]], col[[3]] \
] is returned.\n",
  FontSize->12,
  FontWeight->"Plain"],
 "rgbpalette",
 StyleBox[" [ numshades ] ",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" creates a palette of RGB colors with ",
  FontWeight->"Plain"],
 StyleBox["numshades",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" shades for each basic color. \n",
  FontWeight->"Plain"],
 "testcolors",
 StyleBox[" [ cols ] ",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox["plots all colors in the color list ",
  FontWeight->"Plain"],
 StyleBox["cols",
  FontWeight->"Plain",
  FontSlant->"Italic"],
 StyleBox[" (which can be arbitrarily structured because the list is \
flattened)",
  FontWeight->"Plain"]
}], "Subsection"],

